<%= title "Edit Text Area" %>
<% form_for :form_field_text_area, :url => admin_form_field_text_area_path(@form_field_text_area), :html => { :method => :put } do |f| %>
  <table class='form-table'>
    <%= form_row "Label", f.text_field(:label) %>
    <%= form_row "", submit_tag("Update") %>
  </table>
<% end %>
