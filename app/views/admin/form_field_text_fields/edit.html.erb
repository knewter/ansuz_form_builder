<%= title "Edit Text Field" %>
<% form_for :form_field_text_field, :url => admin_form_field_text_field_path(@form_field_text_field), :html => { :method => :put } do |f| %>
  <table class='form-table'>
    <%= form_row "Label", f.text_field(:label) %>
    <%= form_row "", submit_tag("Update") %>
  </table>
<% end %>
